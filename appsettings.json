{
  "AzureAd": {
    "Scopes": {
      "Read": [ "User.Read", "User.ReadWrite" ],
      "Write": [ "User.ReadWrite" ]
    },
    "AppPermissions": {
      "Read": [ "User.Read.All", "User.ReadWrite.All" ],
      "Write": [ "User.ReadWrite.All" ]
    }
  },
  "Logging": {
    "LogLevel": {
      // MLS 9/14/23 
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "AzureAppServicesFile": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "AzureAppServicesBlob": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft": "Information"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MyCollection": "Server=localhost;database=my_collection;Encrypt=No;Trusted_Connection=True;",
    "Default": "Server=localhost;database=new_my_stuff;Encrypt=No;Trusted_Connection=True;",
    "AZURE_SQL_CONNECTIONSTRING": "Data Source = proof-of-concept-711.database.windows.net; Initial Catalog=poc; Authentication=Active Directory Default; Encrypt=True;"
  },
